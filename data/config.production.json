{
    "url": "<url>",
    "database": {
        "client": "mysql",
        "connection": {
            "host": "<host>",
            "port": 3306,
            "user": "<user>",
            "password": "<password>",
            "database": "<database>",
            "ssl": true
        }
    },
    "fileStorage": false,
    "storage": {
        "active": "ghost-google-drive",
        "ghost-google-drive": {
            "key": {
                "type": "service_account",
                "project_id": "<project_id>",
                "private_key_id": "<private_key_id>",
                "private_key": "<private_key>",
                "client_email": "<client_email>",
                "client_id": "<client_id>",
                "auth_uri": "<auth_uri>",
                "token_uri": "<token_uri>",
                "auth_provider_x509_cert_url": "<auth_provider_x509_cert_url>",
                "client_x509_cert_url": "<client_x509_cert_url>"
              }
        }
    },
    "mail": {
        "from": "'<User>' <no-reply@<domain>.com>",
        "transport": "SMTP",
        "options": {
            "service": "Mailgun",
            "host": "smtp.mailgun.org",
            "port": 587,
            "auth": {
                "user": "postmaster@mail.<domain>.com",
                "pass": "<pass>"
            }
        }
    },
    "server": {
        "host": "0.0.0.0",
        "port": "8080"
    }
}
